<header class="header flex-row">
  <div>
    <%  if back_button  %>
      <%= link_to back_url do %>
        <i class="fas fa-arrow-left"></i>
      <% end %>
    <% end %>
  </div>
  <div class="title">
    <%= title %>
  </div>
  <div>
  
    <% if form_id !='' %>
        <button id="submit_button"onClick="document.getElementById('checked_items').value > 0 ? document.getElementById('<%= form_id %>').submit: false" class='submit' ><%= action_button %></button>
      <% else %>
        <%= link_to destroy_user_session_path, method: :delete do %>
          <i class="fas fa-sign-out-alt"></i>

        <% end %>
    <% end %>
  </div>

</header>
